{
	 "manifest_version": 2,

  "name": "history",
  "description" : "This plugin will show the current mouse position in your screen",
  "version": "1.0",
   "browser_action": {
   
   "default_popup": "popup.html"},

	"content_scripts": [{
    "matches": ["https://*/*"],
    "js":["content.js"]
}]


}